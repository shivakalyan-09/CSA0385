#include <stdio.h>
int sumOfMergedArrays(int num1[], int m, int num2[], int n) 
{
    int i = 0, j = 0, sum = 0;
    while (i < m && j < n) 
    {
        if (num1[i] < num2[j])
        {
            sum =sum+num1[i++];
        } else 
        {
            sum =sum+ num2[j++];
        }
    }
    while (i < m) 
    {
        sum=sum+num1[i++];
    }
    while (j < n) 
    {
        sum=sum+num2[j++];
    }

    return sum;
}
int main() 
{
    int num1[] = {1, 3, 5, 7};  
    int num2[] = {2, 4, 6, 8};  
    int m = sizeof(num1) / sizeof(num1[0]);
    int n = sizeof(num2) / sizeof(num2[0]);
    int totalSum = sumOfMergedArrays(num1, m, num2, n);
    printf("The sum of the merged array is: %d\n", totalSum);
    return 0;
}
