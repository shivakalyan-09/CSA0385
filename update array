#include<stdio.h>
int main()
{
    int i;
    int arr[10]={5,6,7,4,5,3,5,3,2,1};
    printf("original array:");
    for(i=0;i<10;i++)
    {
        printf(" %d ",arr[i]);
    }
    printf("\n");
    arr[3]=10;
    printf("update array:");
    for(i=0;i<10;i++)
    {
        printf(" %d ",arr[i]);
    }
    printf("\n");
    return 0;
}
